   "source": [
    "def jimmy(str):\n",
    "    swap_str=\"\"\n",
    "    for i in range(len(str)):\n",
    "        if i%2==0:\n",
    "            swap_str+=str[i+1]\n",
    "        else:\n",
    "            swap_str+=str[i-1]\n",
    "    return swap_str"
   ]
