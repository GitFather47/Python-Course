   "source": [
    "import turtle\n",
    "turtle.color('red', 'yellow')\n",
    "turtle.begin_fill()\n",
    "while True:\n",
    "    turtle.forward(200)\n",
    "    turtle.left(170)\n",
    "    if abs(turtle.position())<1:\n",
    "        break\n",
    "turtle.end_fill()\n",
    "turtle.done()"
